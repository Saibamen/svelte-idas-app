{
  "name": "svelte-idas-app",
  "version": "0.0.4",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cd ../backend && start dotnet watch && cd ../frontend && vite",
    "dev:open": "cd ../backend && start dotnet watch && cd ../frontend && vite --open",
    "build": "vite build && cd ../backend && dotnet build",
    "preview": "cd ../backend && dotnet run",
    "svelte-check": "svelte-check",
    "svelte-check:watch": "svelte-check --watch",
    "lint": "eslint .",
    "lint:prod": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@gandalan/weblibs": "1.2.1",
    "svelte-routing": "^2.13.0",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.26.5",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-plugin-svelte": "^2.46.1",
    "postcss": "^8.5.1",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.6",
    "svelte-preprocess": "^6.0.3",
    "tailwindcss": "^3.4.17",
    "vite": "^5.4.14"
  }
}
